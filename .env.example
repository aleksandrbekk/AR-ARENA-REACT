# AR ARENA Environment Variables
# Copy this file to .env.local and fill in the values
# NEVER commit .env.local to git!

# Supabase
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key

# Admin (for browser admin access)
VITE_ADMIN_PASSWORD=your-secure-password

# Telegram Bot (for frontend message sending - consider removing)
VITE_BOT_TOKEN=your-bot-token

# ============================================
# VERCEL ENVIRONMENT VARIABLES (set in Vercel Dashboard)
# ============================================

# Supabase (server-side)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Telegram Bots
TELEGRAM_BOT_TOKEN=your-main-bot-token
KIKER_BOT_TOKEN=your-kiker-bot-token

# Lava.top Payments
LAVA_API_KEY=your-lava-api-key
LAVA_WEBHOOK_LOGIN=your-webhook-login
LAVA_WEBHOOK_PASSWORD=your-webhook-password
LAVA_OFFER_ID=your-offer-id

# 0xProcessing
OXPROCESSING_MERCHANT_ID=your-merchant-id

# Toolsy
TOOLSY_SECRET_KEY=your-toolsy-secret
